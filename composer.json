{
    "name": "mediabeastnz/craft-commerce-abandoned-cart",
    "description": "Abandoned Cart plugin for Craft Commerce 2",
    "type": "craft-plugin",
    "version": "1.1.2",
    "keywords": [
        "craft",
        "cms",
        "commerce",
        "craft-plugin",
        "abandoned cart"
    ],
    "support": {
        "docs": "https://github.com/mediabeastnz/craft-commerce-abandoned-cart/blob/master/README.md",
        "issues": "https://github.com/mediabeastnz/craft-commerce-abandoned-cart/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Myles Derham",
            "homepage": "https://github.com/mediabeastnz"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "mediabeastnz\\abandonedcart\\": "src/"
        }
    },
    "extra": {
        "name": "Abandoned Cart for Craft Commerce 2",
        "handle": "abandoned-cart",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/mediabeastnz/craft-commerce-abandoned-cart/master/CHANGELOG.md",
        "class": "mediabeastnz\\abandonedcart\\AbandonedCart"
    }
}
